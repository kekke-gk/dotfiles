{
    "title": "My Dvorak-QWERTY",
    "rules": [
        {
            "description": "Enable Command-[z, x, c, v] shortcuts on Dvorak",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "z",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code":  "z",
                            "modifiers": ["left_command"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "x",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code":  "x",
                            "modifiers": ["left_command"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code":  "c",
                            "modifiers": ["left_command"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "v",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code":  "v",
                            "modifiers": ["left_command"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code":  "q",
                            "modifiers": ["left_command"]
                        }
                    ]
                }
            ]
        }
    ]
}
